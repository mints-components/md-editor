{
  "name": "@mints/component-template",
  "packageManager": "yarn@3.6.1",
  "version": "1.0.0",
  "type": "module",
  "files": [
    "dist",
    "types"
  ],
  "types": "types",
  "main": "./dist/my-component.umd.cjs",
  "module": "./dist/my-component.js",
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/my-component.js"
      },
      "require": {
        "types": "./types/index.d.ts",
        "default": "./dist/my-component.umd.cjs"
      }
    }
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf types",
    "build": "yarn clean && tsc -p tsconfig.build.json && vite build",
    "lint": "eslint . --fix",
    "prettier": "prettier . --write",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublish": "yarn build",
    "publish": "yarn npm publish --access public",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,json,md}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@^6.4.3"
  },
  "dependencies": {
    "@mui/icons-material": "^6.4.5",
    "@mui/material": "^6.4.5",
    "@mui/styled-engine-sc": "^6.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.0.0",
    "styled-components": "^6.1.15"
  },
  "peerDependencies": {
    "@mui/icons-material": "^6.4.5",
    "@mui/material": "^6.4.5",
    "@mui/styled-engine-sc": "^6.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.0.0",
    "styled-components": "^6.1.15"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1",
    "@mints/eslint-config": "^2.0.0",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-onboarding": "^8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/react": "^8.1.11",
    "@storybook/react-vite": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@types/node": "^20.4.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^9.22.0",
    "eslint-plugin-storybook": "^0.11.6",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "pinst": "^3.0.0",
    "prettier": "^3.3.2",
    "storybook": "^8.1.11",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
